<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="net.chenlin.dp.modules.biz.bussiness.dao.YyMRegisterDayMapper">
	
	<select id="listForPage" resultType="net.chenlin.dp.modules.biz.bussiness.entity.YyMRegisterDayEntity">
		SELECT
			`m_register_number`, 
			`create_time`, 
			`id`
		FROM
			yy_m_register_day
		where
		1=1
		<if test="m_register_number != null"> and `m_register_number` = #{m_register_number} </if>
		<if test="create_time != null"> and `create_time` = #{create_time} </if>
		<if test="id != null"> and `id` = #{id} </if>
		ORDER BY
			id DESC
	</select>
	
	<insert id="save">
		INSERT INTO yy_m_register_day (
			`m_register_number`, 
			`create_time`, 
			`id`
		)
		VALUES (
			#{m_register_number}, 
			#{create_time}, 
			#{id}
		)
	</insert>
	
	<select id="getObjectById" resultType="net.chenlin.dp.modules.biz.bussiness.entity.YyMRegisterDayEntity">
		SELECT
			`m_register_number`, 
			`create_time`, 
			`id`
		FROM
			yy_m_register_day
		WHERE
			id = #{id}
	</select>
	
	<update id="update">
		UPDATE yy_m_register_day
	 	<set>
			<if test="m_register_number != null">`m_register_number` = #{m_register_number}, </if>
			<if test="create_time != null">`create_time` = #{create_time}, </if>
		</set>
		WHERE
			id = #{id}
	</update>
	
	<delete id="batchRemove">
		DELETE
		FROM
			yy_m_register_day
		WHERE
			id IN
		<foreach item="id" collection="array" open="(" separator="," close=")">
			#{id}
		</foreach>
	</delete>

</mapper>